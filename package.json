    "material-ui-popup-state": "^2.0.0",
    "mobx": "^6.3.7",
    "mobx-react": "^7.2.1",
    "mobx-utils": "^6.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.0",
    "react-router": "^6.0.2",
    "react-router-dom": "^6.0.2",
    "react-select": "^5.2.1",
    "react-table": "^7.7.0",
    "tss-react": "^3.0.0"
  },
  "devDependencies": {
