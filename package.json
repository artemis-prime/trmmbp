    "generate-unique-id": "^2.0.1",
    "json2mq": "^0.2.0",
    "moment": "^2.29.1",
    "query-string": "^6.13.8",
    "react-linkify": "^1.0.0-alpha",
    "react-player": "^2.7.0",
    "selectn": "^1.1.2"
  },
  "peerDependencies": {
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
